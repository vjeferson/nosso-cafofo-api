    "@types/mime-types": "^2.1.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.7.10",
    "@types/swagger-ui-express": "^4.1.3",
    "jest": "^27.5.1",
    "ts-node-dev": "^1.0.0-pre.56",

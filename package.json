  },
  "homepage": "https://github.com/VJEFERSON/nosso-cafofo-api#readme",
  "dependencies": {
    "@sendgrid/mail": "^7.6.2",
    "@types/pdfmake": "^0.1.21",
    "aws-sdk": "^2.1145.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.11",
    "mime-types": "^2.1.35",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
